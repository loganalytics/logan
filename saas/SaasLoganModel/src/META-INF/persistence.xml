<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
  <persistence-unit name="SaasLoganModel">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:/app/jdbc/jdbc/SaasReposDS</jta-data-source>
    <class>oracle.saas.logan.model.persistance.EmLoganCommonFields</class>
    <class>oracle.saas.logan.model.persistance.EmLoganParser</class>
    <class>oracle.saas.logan.model.persistance.EmLoganParserFieldMap</class>
    <class>oracle.saas.logan.model.persistance.EmTargetTypes</class>
    <class>oracle.saas.logan.model.persistance.rule.EmLoganRule</class>
    <class>oracle.saas.logan.model.persistance.rule.EmLoganRule</class>
    <class>oracle.saas.logan.model.persistance.EmLoganMetaOperator</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="java:/app/jdbc/jdbc/SaasReposDS"/>
    </properties>
  </persistence-unit>
</persistence>
