                                            <af:commandButton id="addSourceRows"
                                                              actionListener="#{viewScope.core_loganalytics_ui_logRuleViewBean.handleAddSources}"
                                                              text="#{coreloganalyticsuiBundle.ADD}"/>
                                            <af:commandButton id="cancelAdd"
