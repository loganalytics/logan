                                                                  styleClass="AFStretchWidth"
                                                                  maxColumns="2">
                                                <af:inputText styleClass="AFDataText"
                                                              label="#{coreloganalyticsuiBundle.TARGET_TYPE}"
                                                              value="#{viewScope.sourcesView.targetTypeDisplayName}"
