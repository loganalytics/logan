                                                        emptyText="#{coreloganalyticsuiBundle.NO_DATA_TO_DISPLAY}"
                                                        rowBandingInterval="1"
                                                        styleClass="AFStretchWidth"
                                                        rowSelection="multiple" 
                                                        id="t55"
                                                        columnStretching="multiple"
                                                        partialTriggers="::comb18"
                                                        autoHeightRows="10"
                                                        contentDelivery="immediate"
                                                        summary="#{coreloganalyticsuiBundle.LIST_OF_LOG_SOURCES_RULE}"
                                                        shortDesc="#{coreloganalyticsuiBundle.LIST_OF_LOG_SOURCES_RULE}">
                                                <af:column rowHeader="true" width="5" id="hcol18"/>
                                                <af:column sortProperty="name" sortable="true" rowHeader="unstyled" 
                                                           headerText="#{coreloganalyticsuiBundle.SOURCE_NAME}"
                                                           id="c89" width="25%">
                                                  <af:outputText value="#{rowfileter.name}" id="ot149"/>
                                                </af:column>
                                                <af:column sortProperty="inclFilePatternsCount"
                                                           headerText="#{coreloganalyticsuiBundle.FILE_PATTERNS}"
                                                           id="c109" width="15%">
                                                  <af:outputText value="#{rowfileter.inclFilePatternsCount}"
                                                                 id="ot139"/>
                                                </af:column>
                                                <af:column sortProperty="description"
                                                           headerText="#{coreloganalyticsuiBundle.DESCRIPTION}"
                                                           id="c49" width="60%">
                                                  <af:outputText value="#{rowfileter.description}"
                                                                 id="ot159"/>
                                                </af:column>
                                              </af:table>
                                            </af:panelGroupLayout>-->
                                          </f:facet>
                                        </af:panelStretchLayout>
                                        <f:facet name="buttonBar">
                                          <af:panelGroupLayout layout="horizontal" halign="end"
